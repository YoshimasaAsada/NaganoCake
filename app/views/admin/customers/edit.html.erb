<div class="container">
<%= form_with model:@end_user, url: admin_customer_path, local:true do |f| %>
<div class="form-group row">
    <div class="col-2">
    <%= f.label :id, "会員ID" %>
    </div>
    <div class="col-1">
    <%= @end_user.id %>
    </div>
    </div>
<div class="form-group row">
    <div class="col-2">
    <%= f.label :last_name, "名前" %>
    </div>
    <div class="col-1">
    <%= f.label :last_name, "(姓)" %>
    </div>
    <div class="col-3">
    <%= f.text_field :last_name, autofocus: true, autocomplete: "last_name", class:"form-control" %>
    </div>
    <div class="col-1">
    <%= f.label :first_name, "(名)" %>
    </div>
    <div class="col-3">
    <%= f.text_field :first_name, autofocus: true, autocomplete: "first_name", class:"form-control" %>
    </div>
  </div>
  
  <div class="form-group row">
    <div class="col-2">
    <%= f.label :last_name, "フリガナ" %>
    </div>
    <div class="col-1">
    <%= f.label :last_name_kana, "(セイ)" %>
    </div>
    <div class="col-3">
    <%= f.text_field :last_name_kana, autofocus: true, autocomplete: "last_name_kana", class:"form-control" %>
    </div>
    <div class="col-1">
    <%= f.label :first_name_kana, "(メイ)" %>
    </div>
    <div class="col-3">
    <%= f.text_field :first_name_kana, autofocus: true, autocomplete: "first_name_kana", class:"form-control" %>
    </div>
  </div>
  
  <div class="form-group row">
    <div class="col-3">
    <%= f.label :email, "メールアドレス" %>
    </div>
    <div class="col-3">
    <%= f.email_field :email, autofocus: true, autocomplete: "email", class:"form-control" %>
    </div>
  </div>
  
  <div class="form-group row">
    <div class="col-3">
    <%= f.label :postal_code, "郵便番号(ハイフンなし)" %>
    </div>
    <div class="col-3">
    <%= f.text_field :postal_code, autofocus: true, autocomplete: "postal_code", class:"form-control" %>
    </div>
  </div>
  
  <div class="form-group row">
    <div class="col-3">
    <%= f.label :address, "住所" %>
    </div>
    <div class="col-7">
    <%= f.text_field :address, autofocus: true, autocomplete: "address", class:"form-control" %>
    </div>
  </div>
  
  <div class="form-group row">
    <div class="col-3">
    <%= f.label :phone_number, "電話番号(ハイフンなし)" %>
    </div>
    <div class="col-3">
    <%= f.text_field :phone_number, autofocus: true, autocomplete: "phone_number", class:"form-control" %>
    </div>
  </div>
  <div class="form-group row">
    <div class="col-3">
    <%= f.label :is_unsubscribed, "会員ステータス" %>
    </div>
    <div class="col-3">
    <%= f.radio_button :is_unsubscribed, :false %>
    <%= f.label :is_unsubscribed, "有効" %>
  　<%= f.radio_button :is_unsubscribed, :true %>
  　<%= f.label :is_unsubscribed, "退会" %>
    </div>
  </div>
  <%= f.submit "変更を保存", class:"btn btn-success" %>
  </div>
  <% end %>
  </div>