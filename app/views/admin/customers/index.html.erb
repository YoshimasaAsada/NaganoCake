
<div class="container">
    <h3>会員一覧</h3><br>
<table class="table">
  <thead>
    <tr>
      <th scope="col">会員ID</th>
      <th scope="col"><氏名</th>
      <th scope="col">メールアドレス</th>
      <th scope="col">ステータス</th>
    </tr>
  </thead>
  <tbody>
      <% @end_users.each do |end_user| %>
    <tr>
      <th scope="row"><%= end_user.id %></th>
      <td><%= link_to admin_customer_path(end_user) do %>
        <%= end_user.last_name %><%= end_user.first_name %>
        <% end %>
      </td>
      <td><%= end_user.email %></td>
      <td>
          <% if end_user.is_unsubscribed == true %>
          退会
          <% else %>
          有効
          <% end %>
      </td>
    </tr>
   <% end %>
  </tbody>
</table>
</div>